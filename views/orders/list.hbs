<h3>
    <p>Order List</p>
    <a href="/orders" class="btn btn-success">
        <i class="fas fa-cart-plus"> </i> New Order
    </a>

    <a href="/product/list" class="btn btn-primary">
        <i class="fas fa-plus-circle"> </i> List Product
    </a>

    <a href="/employee/list" class="btn btn-danger">
        <i class="fas fa-users"> </i> List Employee
    </a>

    <a href="/login" class="btn btn-info">
        <i class="fas fa-sign-out-alt"> </i> Log out
    </a>
</h3>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Quantity</th>
            <th>Total</th>
            <th>OrderDate</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        {{#each list}}
        <tr>
            <td>{{this.Quantity}}</td>
            <td>{{this.Total}}</td>
            <td>{{this.OrderDate}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>